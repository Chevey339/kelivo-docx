#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Door/Kelivo/kelivo-docx/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_76b5afac75e4e67f439e6d51ad8e532e/node_modules/vitepress/bin/node_modules:/mnt/d/Door/Kelivo/kelivo-docx/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_76b5afac75e4e67f439e6d51ad8e532e/node_modules/vitepress/node_modules:/mnt/d/Door/Kelivo/kelivo-docx/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_76b5afac75e4e67f439e6d51ad8e532e/node_modules:/mnt/d/Door/Kelivo/kelivo-docx/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Door/Kelivo/kelivo-docx/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_76b5afac75e4e67f439e6d51ad8e532e/node_modules/vitepress/bin/node_modules:/mnt/d/Door/Kelivo/kelivo-docx/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_76b5afac75e4e67f439e6d51ad8e532e/node_modules/vitepress/node_modules:/mnt/d/Door/Kelivo/kelivo-docx/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_76b5afac75e4e67f439e6d51ad8e532e/node_modules:/mnt/d/Door/Kelivo/kelivo-docx/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../bin/vitepress.js" "$@"
fi
